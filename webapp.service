[Unit]
Description=Webapp Spring Boot application service

[Service]
User=ec2-user
ExecStart=/usr/bin/java -jar /home/ec2-user/webapp/UserWebApp-0.0.1-SNAPSHOT.jar --spring.datasource.url=jdbc:mysql://$dburl:3306 --spring.datasource.username=$dbusername --spring.datasource.password=$dbpassword --spring.jpa.hibernate.ddl-auto=update --aws.s3.bucket.name=$bucketname
ExitStatus=143

TimeoutStopSec=10
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target