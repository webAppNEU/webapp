[Unit]
Description=Webapp Spring Boot application service

[Service]
User=ec2-user
ExecStart=/usr/lib/jvm/java-17-amazon-corretto/java -jar /home/ec2-user/webapp/target/UserWebApp-0.0.1-SNAPSHOT.jar 
ExitStatus=143

TimeoutStopSec=10
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target